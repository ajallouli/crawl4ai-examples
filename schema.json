{
    "name": "Hacker News Articles",
    "baseSelector": "div.body-post.clear",
    "fields": [
        {
            "name": "url",
            "selector": "a.story-link",
            "type": "attribute",
            "attribute": "href"
        },
        {
            "name": "title",
            "selector": "h2.home-title",
            "type": "text"
        },
        {
            "name": "image_url",
            "selector": "div.img-ratio img",
            "type": "attribute",
            "attribute": "src"
        },
        {
            "name": "date",
            "selector": "span.h-datetime",
            "type": "text"
        },
        {
            "name": "tags",
            "selector": "span.h-tags",
            "type": "text"
        },
        {
            "name": "summary",
            "selector": "div.home-desc",
            "type": "text"
        }
    ]
}